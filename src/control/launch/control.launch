<launch>
    <!-- Launch driver to connect the robot-->
    <node name="control_node" pkg="control" type="control_node" output="screen" >
            <!--Load parameters from config files-->
            <rosparam command="load" file="$(find control)/config/params.yaml" />
    </node>

    <!-- Launch PID controller for z movement -->
    <node name="z_move_pid" pkg="pid" type="controller" >
      <param name="Kp" value="1.5" /> <!-- 1.5 -->
      <param name="Ki" value="0.1" /> <!-- 0.1 -->
      <param name="Kd" value="0.001" />   <!-- 0.001 -->
      <param name="upper_limit" value="0.35" />
      <param name="lower_limit" value="-0.35" />
      <param name="windup_limit" value="0.2" />
      <param name="max_loop_frequency" value="100.0" />
      <param name="min_loop_frequency" value="5.0" />
    </node>


</launch>
